<?php

/**
 * @file
 * Contains mm_vocab.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function mm_vocab_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the mm_vocab module.
    case 'help.page.mm_vocab':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Field for OSU media magnet') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function mm_vocab_theme() {
  $theme = [];

  return $theme;
}
